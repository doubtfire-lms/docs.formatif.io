{
  "files.exclude": {
    "node_modules": true,
    ".next": true,
    "out": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "markdown.extension.orderedList.marker": "one",
  "cSpell.words": [
    "AGPL",
    "Affero",
    "Doubtfire",
    "Scrollspy",
    "bulma",
    "editorconfig",
    "fullheight",
    "gapless",
    "highlightjs",
    "hljs",
    "hoverable",
    "jakerenzella",
    "macite",
    "mdast",
    "prettierrc",
    "rehype",
    "unist"
  ]
}
